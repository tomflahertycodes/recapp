<form class= "simple_form">
<%= form_for(@job) do |f| %>
  <div class = label><%= f.label :role %></div>
  <div class = input-box><%= f.text_field :role %> </div>
  <div class = label><%= f.label :location %></div>
  <div class = input-box><%= f.text_field :location%></div>
  <div class = label><%= f.label :contract %></div>
  <div class = input-box><%= f.text_field :contract %></div>
  <div class = label><%= f.label :rate %></div>
  <div class = input-box><%= f.text_field :rate%></div>
  <div class = label><%= f.label :sector %></div>
  <div class = input-box><%= f.text_field :sector %></div>
  <%= f.submit %>
<% end %> </form>
